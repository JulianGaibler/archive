{
  "name": "4x-archive",
  "version": "0.0.1",
  "scripts": {
    "dev": "NODE_PATH=./src dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "debug": "NODE_PATH=./src dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
    "build": "rimraf dist && tsc",
    "doc": "graphdoc -e http://localhost:4000/ -o ./doc/schema",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "knex": "NODE_PATH=./src ts-node ./node_modules/.bin/knex"
  },
  "dependencies": {
    "@2fd/graphdoc": "^2.4.0",
    "apollo-server-express": "^2.15.0",
    "async-mutex": "^0.2.3",
    "bcryptjs": "2.4.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "file-type": "^14.6.2",
    "fluent-ffmpeg": "^2.1.2",
    "fs-capacitor": "^6.2.0",
    "fs-jetpack": "^2.4.0",
    "graphql": "14.5.8",
    "graphql-playground-middleware-express": "^1.7.18",
    "graphql-postgres-subscriptions": "^1.0.5",
    "graphql-relay": "^0.6.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-upload": "^11.0.0",
    "hashids": "^2.2.1",
    "knex": "^0.21.1",
    "morgan": "^1.10.0",
    "node-vibrant": "^3.1.5",
    "objection": "^2.2.0",
    "objection-unique": "^1.2.1",
    "pg": "^8.2.1",
