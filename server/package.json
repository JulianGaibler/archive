    "telegraf": "^3.38.0",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/express": "4.17.3",
    "@types/graphql-upload": "^8.0.3",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.1",
    "@types/node": "14.0.13",
    "@types/sharp": "^0.25.0",
    "@types/tmp": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "dotenv-cli": "3.1.0",
    "eslint": "^7.3.0",
    "eslint-plugin-jsdoc": "^27.1.2",
    "nodemon": "2.0.4",
    "ts-node": "8.10.2",
    "typescript": "3.9.5"
  }
