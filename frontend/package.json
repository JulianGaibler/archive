{
  "name": "archive-frontend",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "dotenv -e ../.env.dev -- astro dev",
    "build": "dotenv -e ../.env.prod -- astro build",
    "start": "dotenv -e ../.env.prod -- node ./dist/server/entry.mjs",
    "preview": "dotenv -e ../.env.dev -- astro preview",
    "astro": "dotenv -e ../.env.dev -- astro",
    "lint": "eslint src/",
    "prettier": "prettier --write --check 'src/**/*.*'",
    "generate": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@astrojs/node": "^9.3.0",
    "@astrojs/svelte": "^7.1.0",
    "@types/cookie": "^0.6.0",
    "astro": "^5.11.1",
    "cookie": "^1.0.2",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^6.0.6",
    "sass": "^1.89.2",
    "svelte": "^5.35.6",
    "tint": "git://github.com/JulianGaibler/tint.git",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.7",
    "@graphql-codegen/client-preset": "^4.8.3",
    "@graphql-codegen/typescript": "4.1.6",
    "@graphql-codegen/typescript-graphql-request": "^6.3.0",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-codegen/typescript-resolvers": "^4.5.1",
    "dotenv-cli": "8.0.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-svelte": "^3.10.1",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-jsdoc": "^1.3.3",
    "prettier-plugin-svelte": "^3.4.0",
    "typescript-eslint": "^8.36.0",
    "vite-plugin-graphql-loader": "^4.0.4"
  }
}
