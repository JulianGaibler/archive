    "lint": "eslint src/",
    "prettier": "prettier --write --check 'src/**/*.*'",
    "generate": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@astrojs/node": "^9.5.2",
    "@astrojs/svelte": "^7.2.5",
    "@types/cookie": "^0.6.0",
    "astro": "^5.16.11",
    "cookie": "^1.1.1",
    "graphql": "^16.12.0",
    "graphql-request": "^7.4.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^6.0.6",
    "sass": "^1.97.2",
    "svelte": "^5.46.4",
    "tint": "git://github.com/JulianGaibler/tint.git",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "6.1.1",
    "@graphql-codegen/client-preset": "^5.2.2",
    "@graphql-codegen/typescript": "5.0.7",
    "@graphql-codegen/typescript-graphql-request": "^6.4.0",
